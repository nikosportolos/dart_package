{
  "name": "dart_package",
  "version": "0.2.2",
  "description": "Optimize your Dart & Flutter package development workflow with the [**dart_package**](https://github.com/marketplace/actions/dart-package) GitHub Action.",
  "author": "Nikos Portolos",
  "license": "MIT",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:check": "eslint . --exit-on-fatal-error",
    "audit": "npm audit --dry-run --audit-level=low"
  },
  "keywords": [
    "dart",
    "flutter",
    "action",
    "cicd",
    "codecov",
    "github-actions"
  ],
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "eslint": "^9.16.0",
    "eslint-plugin-json-format": "^2.0.1",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "prettier-plugin-packagejson": "^2.5.20",
    "prettier-plugin-sh": "^0.18.0"
  }
}
